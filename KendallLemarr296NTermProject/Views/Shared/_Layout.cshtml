<!DOCTYPE html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - KendallLemarr TermProject</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body class="">
    <section class="">
        <div class="text-monospace bg-dark text-light">
            <div class="row">
                <div class="col-sm-1">
                </div>
                <div class="col-sm-10">
                    <main role="main" class="pb-3 text-light">
                        @RenderBody()
                    </main>
                </div>
                <div class="col-sm-1">
                </div>
            </div>
        </div>
        </section>
        <section>
                <div class="footer bg-dark text-light">
                    <div class="text-center">
                        <a class="btn btn-outline-light text-light" asp-area="" asp-controller="Home" asp-action="Index"><i>Button one</i></a>
                        <a class="btn btn-outline-light text-light" asp-area="" asp-controller="Home" asp-action="Index"><i>Button two</i></a>
                        <a class="btn btn-outline-light text-light" asp-area="" asp-controller="Home" asp-action="Index"><i>Button three</i></a>
                        <a class="btn btn-outline-light text-light" asp-area="" asp-controller="Home" asp-action="Index"><i>Button four</i></a>
                    </div>
                </div>
        </section>
        <script src="~/lib/jquery/dist/jquery.min.js"></script>
        <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
        <script src="~/js/site.js" asp-append-version="true"></script>
        @RenderSection("Scripts", required: false)
</body>

